<% if(@users.any?) %>
  <ul>
    <% @users.each do |user| %>
      <li><%= user.name %></li>
    <% end %>
  </ul>
<% end %>
<% if(@reviews.any?) %>
  <ol>
    <% @reviews.each do |review| %>
      <li><strong><%= product_title(review.product_id) %>: </strong>"<em><%= review.description %></em>"</li>
    <% end %>
  </ol>
<% end %>